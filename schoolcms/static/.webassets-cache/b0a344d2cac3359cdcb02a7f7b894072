V/** @jsx React.DOM */\u000a\u000aSC.App = React.createClass({displayName: 'App',\u000a  mixins: [ReactMiniRouter.RouterMixin],\u000a  routes: {\u000a      '/': 'indexHandler',\u000a      '/login': 'loginHandler',\u000a      '/logout': 'logoutHandler',\u000a      '/admin/adduser': 'adduserHandler',\u000a      '/announce': 'annIndexHandler',\u000a      '/announce/:annid': 'announceHandler',\u000a      '/announce/edit/:annid': 'editAnnHandler',\u000a      '/group': 'groupHandler',\u000a      '/grouplist': 'groupListHandler',\u000a  },\u000a  ajax: function(){\u000a    var xhr = new XMLHttpRequest();\u000a    var url = '/api'+window.location.pathname+window.location.search;\u000a    xhr.open('GET',url,false);\u000a    xhr.upload.onprogress = function(e){\u000a      if(e.lengthComputable){\u000a        this.setState({loading: e.loaded*100 / e.total});\u000a      }\u000a    }.bind(this);\u000a    xhr.onerror = function(){\u000a      this.setState({status: 500});\u000a    };\u000a    xhr.send();\u000a    if(xhr.status){\u000a      this.setState({status:xhr.status});\u000a    }\u000a    this.setState({loading: -1});\u000a    return (xhr.status==200?JSON.parse(xhr.response):{});\u000a  },\u000a  getInitialState: function() {\u000a    return {\u000a      data: {},\u000a      loading: -1,\u000a      status: 200,\u000a    };\u000a  },\u000a  render: function() {\u000a    if(this.state.status==200){\u000a      return (\u000a        React.createElement("div", null, \u000a          React.createElement(RB.ProgressBar, {now: this.state.precent, bsStyle: "info", \u000a              style: {position:'fixed',top:'0px',height:'4px',width:'100%'}}), \u000a          React.createElement(SC.NavbarInstance, {loginpage: _data.loginpage, current_user: _data.current_user, _xsrf_token: _data._xsrf_token}), \u000a          this.renderCurrentRoute()\u000a        )\u000a      );\u000a    }else{\u000a      return (\u000a        React.createElement("h1", null, "Geez, ", this.status)\u000a      );\u0